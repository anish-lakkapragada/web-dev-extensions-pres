{
    "manifest_version": 3,
    "name": "Web Devvie",
    "version": "69.420",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["contentScript.js"]
        }
    ],
    "action": {
        "default_popup": "popup.html"
    }, 
    "permissions": ["tabs", "storage"]
}  